{{% import time %}}

$TTL    604800
@       IN      SOA     X.x.X.  (
        {% int(time.time()) %}      ; Serial
        604800  ; Refresh
        86400   ; Retry
        2419200 ; Expire
        604800  ; Negative Cache TTL
)

; name servers

{% for r in dns_records %}

	{% r.name %}     IN     {% r.type %}     {% r.value %} 

{% endfor %}


; test
test            IN      A       1.2.3.4
